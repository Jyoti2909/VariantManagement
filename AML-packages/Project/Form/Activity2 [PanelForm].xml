      <html_code><![CDATA[<script type="text/javascript">
function getTabbar_inRelationships()
{
  var itm = document.item;
  var isFromProjectTemplate = itm.selectSingleNode("ancestor::Item[@type='Project Template']");
  if (!isFromProjectTemplate) return;
  var a = top.aras;
  
  var d = a.uiGenerateRelationshipsTabbar("Activity2", itm.getAttribute("id"));
  var nms2Use = new Array("Activity2 Assignment", "Activity2 Task", "Predecessor", "Activity2 Deliverable");
  var nds = d.selectNodes("/tabbar/tab[not(@type_name='"+nms2Use.join("' or @type_name='")+"')]");
  for (var i=0; i<nds.length; i++)
  {
    nds[i].parentNode.removeChild(nds[i]);
  }
  return d;
}
if (!parent.RelationshipsOverriddenFunctions) parent.RelationshipsOverriddenFunctions = new Object();
parent.RelationshipsOverriddenFunctions["getTabbar"] = getTabbar_inRelationships;
</script>]]></html_code>
