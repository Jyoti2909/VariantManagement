					<Message>
						<event name='ids_modified' value='' />
					</Message>
				</SOAP-ENV:Body>
			</SOAP-ENV:Envelope>
		</Baseline>
		<Comparison>
			<Ignore xpath='Baseline/SOAP-ENV:Envelope/SOAP-ENV:Body/Message/event/@value' />
		</Comparison>
	</Request>
	<TestCaseCleanUp runAs='root:innovator'>
