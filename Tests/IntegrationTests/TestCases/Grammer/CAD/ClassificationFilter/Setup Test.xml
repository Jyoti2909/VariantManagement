			<Variable id="Password.Hash.Of.1" action="apply" xpath="Query/AML/Item/password | Query/AML/Item/confirm_password" />
		</Variables>
		<Query>
			<AML>
				<Item type="User" id="7C50A99EDC464B96BE1DF43501CA7994" action="add" doGetItem="0">
