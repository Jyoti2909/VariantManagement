<?xml version="1.0" encoding="utf-8"?>
<toolbarapplet on_click="onToolbarButtonClick" on_load="onToolbarLoad" buttonstyle="windows" buttonsize="26,25" font="Arial-8">
	<toolbar id="tb1">
		<drop_down_button id="view_panels_menu" disabled="true" text="Anzeigen" tooltip="Zusätzliche Ebenen einblenden"/>
		<choice id="saved_search_project_tree" label="Gespeicherte Suchen:"/>
		<separator/>
		<drop_down_button id="actions_menu" text="Aktionen"/>
		<separator/>
		<button image="../images/ProjectExpandAll.svg" id="expand_all" tooltip="Alles aufklappen" text="Alles aufklappen"/>
		<button image="../images/ProjectCollapseAll.svg" id="collapse_all" tooltip="Alles zuklappen" text="Alles zuklappen"/>
		<separator/>

		<button disabled="true" image="../images/AddWBSElement.svg" id="add_wbs_element" tooltip="WBS hinzufügen" text="WBS hinzufügen"/>
		<button disabled="true" image="../images/AddActivity.svg" id="add_activity" tooltip="Aktivität hinzufügen" text="Aktivität hinzufügen"/>
		<button disabled="true" image="../images/AddMilestone.svg" id="add_milestone" tooltip="Meilenstein hinzufügen" text="Meilenstein hinzufügen"/>
		<separator/>
		<button disabled="true" image="../images/UnIndent.svg" id="outdent" tooltip="Rausrücken" text="Rausrücken"/>
		<button disabled="true" image="../images/Indent.svg" id="indent" tooltip="Einrücken" text="Einrücken"/>
		<button disabled="true" image="../images/NextPage.svg" id="move_down" tooltip="Seite runter" text="Seite runter"/>
		<button disabled="true" image="../images/PreviousPage.svg" id="move_up" tooltip="Seite hoch" text="Seite hoch"/>
		<separator/>
		<button disabled="true" image="../images/Copy.svg" id="copy" tooltip="Kopieren" text="Kopieren"/>
		<button disabled="true" image="../images/Cut.svg" id="cut" tooltip="Ausschneiden" text="Ausschneiden"/>
		<button disabled="true" image="../images/Paste.svg" id="paste" tooltip="Einfügen" text="Einfügen"/>
		<separator/>
		<button disabled="true" image="../images/Deliverable.svg" id="add_existing_deliverable" tooltip="Vorhandenes Ergebnisobjekt hinzufügen" text="Vorhandenes Ergebnisobjekt hinzufügen"/>
		<button disabled="true" image="../images/NewDeliverable.svg" id="add_new_deliverable" tooltip="Neues Ergebnisobjekt hinzufügen" text="Neues Ergebnisobjekt hinzufügen"/>
		<separator/>
		<button image="../images/EditItem.svg" id="view_edit_selected_item" tooltip="Ausgewähltes Objekt anzeigen/bearbeiten" text="Ausgewähltes Objekt anzeigen/bearbeiten"/>
		<button disabled="true" image="../images/Delete.svg" id="delete_selected_item" tooltip="Ausgewähltes Objekt löschen" text="Ausgewähltes Objekt löschen"/>
		<button image="../images/Schedule.svg" id="execute_scheduling" tooltip="Planberechnung starten" text="Planberechnung starten"/>
		<button disabled="true" image="../images/UpdateActivity.svg" id="open_completion_dialog" tooltip="Rückmeldedialog öffnen" text="Rückmeldedialog öffnen"/>
		<button disabled="false" image="../images/Print.svg" id="print" tooltip="Drucken" text="Drucken"/>
		<button image="../images/GanttChart.svg" id="show_gantt_chart" tooltip="Gantt-Chart anzeigen" text="Gantt-Chart anzeigen"/>
		<button image="../images/ExecuteSearch.svg" id="execute_filter" tooltip="Filter anwenden" text="Filter anwenden"/>
		<button image="../images/ClearSearchCriteria.svg" id="clear_filter" tooltip="Filter zurücksetzen" text="Filter zurücksetzen"/>
		<separator />
		<choice id="gantt_time_delta" label="Timeline:" invisible="true">
			<choiceitem id="day">Day</choiceitem>
			<choiceitem id="week">Week</choiceitem>
			<choiceitem id="month">Month</choiceitem>
			<choiceitem id="quarter">Quarter</choiceitem>
			<choiceitem id="year">Year</choiceitem>
		</choice>
		<button state="true" image="../images/ShowDependencyLinks.svg" id="gantt_dependency_links" tooltip="Abhängigkeitslinks anzeigen" text="Abhängigkeitslinks anzeigen" invisible="true" />
		<button state="true" image="../images/ShowCompletionProgress.svg" id="gantt_completion_progress" tooltip="Vollendungsfortschritt anzeigen" text="Vollendungsfortschritt anzeigen" invisible="true" />
		<button state="true" image="../images/ShowTaskFloat.svg" id="gantt_task_float" tooltip="Aufgabenfloat anzeigen" text="Aufgabenfloat anzeigen" invisible="true" />
	</toolbar>
</toolbarapplet>
