<?xml version="1.0" encoding="utf-8"?>
<toolbarapplet on_click="onToolbarButtonClick" on_load="onToolbarLoad" buttonstyle="windows" buttonsize="26,25" font="Arial-8">
	<toolbar id="tb1">
		<drop_down_button id="view_panels_menu" disabled="true" text="表示" tooltip="追加ビューパネルの表示"/>
		<choice id="saved_search_project_tree" label="保存検索:"/>
		<separator/>
		<drop_down_button id="actions_menu" text="アクション"/>
		<separator/>
        <button image="../images/ProjectExpandAll.svg" id="expand_all" tooltip="全て展開" text="全て展開"/>
		<button image="../images/ProjectCollapseAll.svg" id="collapse_all" tooltip="全て折りたたみ" text="全て折りたたみ"/>
		<separator/>

		<button disabled="true" image="../images/AddWBSElement.svg" id="add_wbs_element" tooltip="WBSエレメントの追加" text="WBSエレメントの追加"/>
		<button disabled="true" image="../images/AddActivity.svg" id="add_activity" tooltip="アクティビティの追加" text="アクティビティの追加"/>
		<button disabled="true" image="../images/AddMilestone.svg" id="add_milestone" tooltip="マイルストーンの追加" text="マイルストーンの追加"/>
		<separator/>
		<button disabled="true" image="../images/UnIndent.svg" id="outdent" tooltip="レベル上げ" text="レベル上げ"/>
		<button disabled="true" image="../images/Indent.svg" id="indent" tooltip="レベル下げ" text="レベル下げ"/>
		<button disabled="true" image="../images/NextPage.svg" id="move_down" tooltip="下へ移動" text="下へ移動"/>
		<button disabled="true" image="../images/PreviousPage.svg" id="move_up" tooltip="上へ移動" text="上へ移動"/>
		<separator/>
		<button disabled="true" image="../images/Copy.svg" id="copy" tooltip="コピー" text="コピー"/>
		<button disabled="true" image="../images/Cut.svg" id="cut" tooltip="切り取り" text="切り取り"/>
		<button disabled="true" image="../images/Paste.svg" id="paste" tooltip="貼り付け" text="貼り付け"/>
		<separator/>
		<button disabled="true" image="../images/Deliverable.svg" id="add_existing_deliverable" tooltip="既存成果物の追加" text="既存成果物の追加"/>
		<button disabled="true" image="../images/NewDeliverable.svg" id="add_new_deliverable" tooltip="新規成果物の追加" text="新規成果物の追加"/>
		<separator/>
		<button image="../images/EditItem.svg" id="view_edit_selected_item" tooltip="選択アイテムの表示/編集" text="選択アイテムの表示/編集"/>
		<button disabled="true" image="../images/Delete.svg" id="delete_selected_item" tooltip="選択アイテムの削除" text="選択アイテムの削除"/>
		<button image="../images/Schedule.svg" id="execute_scheduling" tooltip="スケジューラー実行" text="スケジューラー実行"/>
		<button disabled="true" image="../images/UpdateActivity.svg" id="open_completion_dialog" tooltip="完了ダイアログの表示" text="完了ダイアログの表示"/>
		<button disabled="false" image="../images/Print.svg" id="print" tooltip="印刷" text="印刷"/>
		<button image="../images/GanttChart.svg" id="show_gantt_chart" tooltip="ガントチャート表示" text="ガントチャート表示"/>
		<button image="../images/ExecuteSearch.svg" id="execute_filter" tooltip="検索実行" text="検索実行"/>
		<button image="../images/ClearSearchCriteria.svg" id="clear_filter" tooltip="検索条件のクリア" text="検索条件のクリア"/>
		<separator />
		<choice id="gantt_time_delta" label="タイムライン：" invisible="true">
			<choiceitem id="day">日</choiceitem>
			<choiceitem id="week">週</choiceitem>
			<choiceitem id="month">月</choiceitem>
			<choiceitem id="quarter">四半期</choiceitem>
			<choiceitem id="year">年</choiceitem>
		</choice>
		<button state="true" image="../images/ShowDependencyLinks.svg" id="gantt_dependency_links" tooltip="依存関係リンクを表示する" text="依存関係リンクを表示する" invisible="true" />
		<button state="true" image="../images/ShowCompletionProgress.svg" id="gantt_completion_progress" tooltip="完了進捗状況を表示する" text="完了進捗状況を表示する" invisible="true" />
		<button state="true" image="../images/ShowTaskFloat.svg" id="gantt_task_float" tooltip="タスクフロートを表示する" text="タスクフロートを表示する" invisible="true" />
	</toolbar>
</toolbarapplet>